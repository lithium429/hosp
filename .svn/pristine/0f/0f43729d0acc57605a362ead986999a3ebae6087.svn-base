package com.xz.oa.core.domain.entity;

import java.io.Serializable;
import com.xz.base.domain.BaseEntity;

public class AdviceConfig extends BaseEntity implements Serializable {

	private static final long serialVersionUID = -1L;
	

	/**
	 * id
	 */
	private java.lang.Integer id;

	/**
	 * instruction
	 */
	private java.lang.String instruction;

	/**
	 * regulation
	 */
	private java.lang.String regulation;

	public AdviceConfig() {
	}
	
	public void setId(java.lang.Integer id) {
		this.id = id;
	}

	public java.lang.Integer getId() {
		return this.id;
	}
	
	public void setInstruction(java.lang.String instruction) {
		this.instruction = instruction;
	}

	public java.lang.String getInstruction() {
		return this.instruction;
	}
	
	public void setRegulation(java.lang.String regulation) {
		this.regulation = regulation;
	}

	public java.lang.String getRegulation() {
		return this.regulation;
	}


}