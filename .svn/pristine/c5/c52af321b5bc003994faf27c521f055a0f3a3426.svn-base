package com.xz.base.utils;

public class BizUtil {
	/**
	 * @Description 根据星期几获取排班时间段类型 
	 * @param dayOfWeek
	 * @return Byte     
	 */
	public static Byte getTimeTypeByDayOfWeek(Integer dayOfWeek){
		// 周一到周五
		if(dayOfWeek <= 5){
			return 1;
		}
		// 周六周日
		else {
			return 2;
		}
	}
}
