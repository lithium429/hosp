package com.xz.oa.core.domain.entity;

import java.io.Serializable;
import java.util.List;

import com.xz.base.domain.BaseEntity;

public class PublicityColumn extends BaseEntity implements Serializable {

	private static final long serialVersionUID = -1L;

	/**
	 * id
	 */
	private java.lang.Integer id;

	/**
	 * type
	 */
	private java.lang.Integer type;

	/**
	 * title
	 */
	private java.lang.String title;

	/**
	 * content
	 */
	private java.lang.String content;

	/**
	 * sort
	 */
	private java.lang.Integer sort;

	/**
	 * creator_id
	 */
	private java.lang.Integer creator_id;

	/**
	 * create_time
	 */
	private java.util.Date create_time;

	/**
	 * 附件
	 */
	private List<PublicityColumnFile> files;

	public PublicityColumn() {
	}
	
	public PublicityColumn(Integer type) {
		this.type = type;
	}

	public void setId(java.lang.Integer id) {
		this.id = id;
	}

	public java.lang.Integer getId() {
		return this.id;
	}

	public void setType(java.lang.Integer type) {
		this.type = type;
	}

	public java.lang.Integer getType() {
		return this.type;
	}

	public void setTitle(java.lang.String title) {
		this.title = title;
	}

	public java.lang.String getTitle() {
		return this.title;
	}

	public void setContent(java.lang.String content) {
		this.content = content;
	}

	public java.lang.String getContent() {
		return this.content;
	}

	public void setSort(java.lang.Integer sort) {
		this.sort = sort;
	}

	public java.lang.Integer getSort() {
		return this.sort;
	}

	public void setCreator_id(java.lang.Integer creator_id) {
		this.creator_id = creator_id;
	}

	public java.lang.Integer getCreator_id() {
		return this.creator_id;
	}

	public void setCreate_time(java.util.Date create_time) {
		this.create_time = create_time;
	}

	public java.util.Date getCreate_time() {
		return this.create_time;
	}

	public List<PublicityColumnFile> getFiles() {
		return files;
	}

	public void setFiles(List<PublicityColumnFile> files) {
		this.files = files;
	}
}